<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="900.0" prefWidth="1280.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Gui.addCategorieController">
    <stylesheets>
        <URL value="@categorie.css" />
    </stylesheets>
   <children>
      <Pane prefHeight="900.0" prefWidth="1280.0" style="-fx-background-color: #1d0761;">
         <children>
            <VBox alignment="TOP_CENTER" layoutX="124.0" layoutY="101.0" prefHeight="618.0" prefWidth="965.0" spacing="50.0" style="-fx-padding: 20;">
               <children>
                  <TextField fx:id="nomCategorie" prefHeight="50.0" prefWidth="1200.0" promptText="Nom Categorie" style="-fx-background-color: null; -fx-border-color: white; -fx-border-radius: 3em;">
                     <cursor>
                        <Cursor fx:constant="TEXT" />
                     </cursor>
                     <font>
                        <Font size="20.0" />
                     </font>
                  </TextField>
                  <TextField fx:id="descriptionCategorie" prefHeight="50.0" prefWidth="1200.0" promptText="Description" style="-fx-border-radius: 3em; -fx-border-color: white; -fx-background-color: null;">
                     <font>
                        <Font size="20.0" />
                     </font></TextField>
                  <HBox prefHeight="50.0" prefWidth="298.0" spacing="10.0">
                     <children>
                        <TextField fx:id="imageCategorie" prefHeight="50.0" prefWidth="786.0" promptText="ImageCategorie" style="-fx-background-color: null; -fx-border-color: white; -fx-border-radius: 3em;">
                           <font>
                              <Font size="20.0" />
                           </font></TextField>
                        <Button fx:id="uploadCategorie" mnemonicParsing="false" prefHeight="50.0" prefWidth="130.0" style="-fx-background-color: #f04086; -fx-background-radius: 3em;" text="Upload" textFill="WHITE">
                           <cursor>
                              <Cursor fx:constant="HAND" />
                           </cursor>
                           <font>
                              <Font size="20.0" />
                           </font>
                        </Button>
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" prefHeight="67.0" prefWidth="925.0" spacing="30.0">
                     <children>
                        <Button fx:id="annulerCategorie" mnemonicParsing="false" prefHeight="50.0" prefWidth="130.0" style="-fx-background-radius: 3em; -fx-background-color: #3d09ac;" text="Annuler" textFill="WHITE">
                           <font>
                              <Font size="20.0" />
                           </font></Button>
                        <Button fx:id="addCategorie" mnemonicParsing="false" prefHeight="50.0" prefWidth="130.0" style="-fx-background-color: #f04086; -fx-background-radius: 3em;" text="Ajouter" textFill="WHITE">
                           <font>
                              <Font size="20.0" />
                           </font></Button>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Pane>
   </children>
</AnchorPane>
