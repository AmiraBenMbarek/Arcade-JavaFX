<AnchorPane id="AnchorPane" fx:id="contentCategorie" prefHeight="900.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="arcade.Gui.addCategorieController">
