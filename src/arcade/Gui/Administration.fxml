<AnchorPane id="AnchorPane" prefHeight="550.0" prefWidth="1272.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="arcade.Gui.AdministrationController">
    <stylesheets>
        <URL value="@administration.css" />
    </stylesheets>
   <children>
      <BorderPane fx:id="BorderPane" layoutX="-3.0" layoutY="14.0" prefHeight="552.0" prefWidth="1272.0" style="-fx-background-color: WHITE;">
         <left>
            <VBox prefHeight="802.0" prefWidth="214.0" style="-fx-background-color: #f2f2f2;" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="btnuser" alignment="BASELINE_LEFT" graphicTextGap="10.0" mnemonicParsing="false" onAction="#btnuser" prefHeight="70.0" prefWidth="270.0" styleClass="button1" stylesheets="@administration.css" text="Users" textFill="WHITE">
                     <padding>
                        <Insets left="50.0" />
                     </padding>
                     <graphic>
                        <FontAwesomeIconView fill="WHITE" glyphName="USERS" size="16" text="" />
                     </graphic>
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Button>
                  <Button fx:id="btnadduser" alignment="BASELINE_LEFT" graphicTextGap="10.0" mnemonicParsing="false" onAction="#btnadduser" prefHeight="70.0" prefWidth="258.0" styleClass="button1" stylesheets="@administration.css" text="Add User" textFill="WHITE">
                     <padding>
                        <Insets left="50.0" />
                     </padding>
                     <graphic>
                        <FontAwesomeIconView fill="WHITE" glyphName="CALENDAR" text="" />
                     </graphic>
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Button>
               </children>
            </VBox>
         </left>
         <top>
            <HBox alignment="TOP_RIGHT" prefHeight="145.0" prefWidth="1272.0" style="-fx-background-color: #723E64;" stylesheets="@administration.css" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="logout" alignment="TOP_LEFT" graphicTextGap="15.0" mnemonicParsing="false" onAction="#logout" prefHeight="55.0" prefWidth="199.0" style="-fx-background-color: #3a3289;" styleClass="mainFxmlClass" text="Déconnexion" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="18.0" />
                     </font>
                     <graphic>
                        <Circle fx:id="ProfileCircle" fill="#ffffffab" radius="21.0" stroke="WHITE" strokeType="INSIDE" />
                     </graphic>
                  </Button>
               </children></HBox>
         </top>
         <center>
            <Pane fx:id="ghada" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
         </center>
      </BorderPane>
      <HBox alignment="CENTER_LEFT">
         <children>
            <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@logo.png" />
               </image>
            </ImageView>
         </children>
      </HBox>
   </children>
</AnchorPane>
