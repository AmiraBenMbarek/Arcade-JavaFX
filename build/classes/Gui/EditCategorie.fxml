<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="900.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Gui.EditCategorieController">
   <children>
      <VBox alignment="TOP_CENTER" fillWidth="false" layoutX="111.0" layoutY="119.0" prefHeight="603.0" prefWidth="894.0" spacing="10.0">
         <children>
            <TextField fx:id="nomCategorie" prefHeight="75.0" prefWidth="488.0" promptText="Nom Categorie" style="-fx-background-color: null; -fx-background-radius: 2em; -fx-border-color: #b7c3d7; -fx-border-radius: 2em;">
               <VBox.margin>
                  <Insets top="30.0" />
               </VBox.margin>
               <cursor>
                  <Cursor fx:constant="TEXT" />
               </cursor>
            </TextField>
            <Label fx:id="nomCategorieError" textFill="#c20b0b" />
            <TextField fx:id="description" prefHeight="68.0" prefWidth="494.0" promptText="Description" style="-fx-background-color: null; -fx-background-radius: 2em; -fx-border-color: #b7c3d7; -fx-border-radius: 2em;">
               <cursor>
                  <Cursor fx:constant="TEXT" />
               </cursor>
            </TextField>
            <Label fx:id="descriptionError" textFill="#c20b0b" />
            <HBox alignment="CENTER" prefHeight="66.0" prefWidth="367.0" spacing="20.0">
               <children>
                  <Button fx:id="cancel" lineSpacing="3.0" mnemonicParsing="false" onAction="#cancel" prefHeight="44.0" prefWidth="117.0" style="-fx-background-color: #6610f2; -fx-background-radius: 2em; -fx-cursor: hand;" text="cancel" textFill="#e7e5e5">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Button>
                  <Button fx:id="editCategorie" lineSpacing="3.0" mnemonicParsing="false" onAction="#editCategorie" prefHeight="46.0" prefWidth="112.0" style="-fx-background-color: #fd8a16; -fx-background-radius: 2em; -fx-cursor: hand;" text="Edit" textFill="#e7e5e5">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </VBox>
   </children>
</AnchorPane>
